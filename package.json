    "node-sass": "^9.0.0",
    "puppeteer": "^22.6.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.4",
    "vue": "^3.4.21",
    "vue-loader": "^17.4.2",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
